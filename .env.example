# 服务器配置
NODE_ENV=production
PORT=7001

# 数据库配置
DB_HOST=mysql
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=123456
DB_DATABASE=py_ques
DB_SYNCHRONIZE=true
DB_LOGGING=true

# JWT配置
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=1h

# Coze API配置
COZE_CLIENT_ID=your-client-id
COZE_API_BASE=https://api.coze.cn
COZE_WWW_BASE=https://www.coze.cn
COZE_PUBLIC_KEY_ID=your-public-key-id
# 注意: 私钥内容应该放在单独的文件中，这里仅作示例
COZE_PRIVATE_KEY_PATH=./private_key.pem

# 邮件服务配置
EMAIL_HOST=smtp.163.com
EMAIL_PORT=465
EMAIL_SECURE=true
EMAIL_USER=your-email@163.com
EMAIL_PASS=your-email-password

# 验证码配置
VERIFICATION_CODE_EXPIRE_TIME=300

# Coze工作流配置
COZE_WORKFLOW_ID=your-workflow-id 